{
  "version": "5.0.0-ALPHA",
  "name": "lambda-lib",
  "description": "Decorators and tools for AWS Lambda",
  "keywords": [
    "lambda",
    "aws",
    "promise",
    "decorator"
  ],
  "author": "Steven Inskip",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SkippyZA/lambda-lib.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run test && npm run build",
    "pretest": "tslint -c tslint.json --project tsconfig.json 'src/**/*.ts'",
    "coverage": "nyc mocha",
    "test": "mocha"
  },
  "pre-push": [
    "test"
  ],
  "dependencies": {
    "circular-json": "0.5.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.17",
    "@types/chai": "4.1.7",
    "@types/mocha": "5.2.5",
    "@types/node": "10.12.12",
    "@types/sinon": "5.0.7",
    "@types/sinon-chai": "3.2.1",
    "chai": "4.2.0",
    "mocha": "5.2.0",
    "nodemon": "1.18.9",
    "nyc": "13.1.0",
    "pre-push": "0.1.1",
    "sinon": "7.1.1",
    "sinon-chai": "3.3.0",
    "source-map-support": "0.5.9",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-standard": "8.0.1",
    "typescript": "3.2.1"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "all": true,
    "reporter": [
      "lcov",
      "text",
      "json"
    ]
  }
}
